
 <a href="/">Ir al formulario -></a>
 {{#if Arreglodeproductos}}

     <h2> Listado de productos</h2> 
     <table>
        <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Precio</th>
            <th>thumbnail</th>
        </tr>
        {{#each Arreglodeproductos }}
            <tr>
                <td> {{this.id}}</td>
                <td>{{this.title}} </td>
                <td> {{this.price}} </td>
                <td> <img src= {{this.thumbnail}} width=100  alt=""> </td>
            </tr>
        {{/each}}

     </table>
    
 {{ else }}
    <h2> No hay datos</h2>
 {{/if}}
    